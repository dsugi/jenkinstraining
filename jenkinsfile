pipeline {
    agent any

    stages {
        stage('Step One') {
            steps {
               bat 'dir c:\\windows > g:\\training\\hasil.txt'
            }
        }
        stage('Step Two') {
            steps {
               bat 'copy hasil.txt g:\\training\\sample\\'
            }
        }
        stage('Step Three') {
            steps {
               bat 'del g:\\training\\hasil.txt'
               bat 'cd g:\\training\\sample'
               bat 'ren hasil.txt result.txt'
            }
        }
    }
}